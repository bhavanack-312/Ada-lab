#include<stdio.h>
#include<conio.h>
#include<math.h>
int gcd(int m,int n)
{
if(n==0) return m;
if(m<n) return gcd(n,m);
return gcd(n,m%n);
}

void main()
{
int res ,m,n;
clrscr();
printf("enter the values of m and n\n");
scanf("%d %d",&m,&n);
res = gcd(m,n);
printf("gcd(%d,%d)=%d\n",m,n,res);
getch();
}
