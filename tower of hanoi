#include<stdio.h>
#include<conio.h>
void tower(int n,int source,int temp,int dest)
{
if(n==1)
{
printf("move disc %d from %c to %c\n",n,source,dest);
return;
}
tower(n-1,source,dest,temp);
printf("move disc %d from %c to %c\n",n,source,dest);
tower(n-1,temp,source,dest);
}
void main()
{
int n;
clrscr();
printf("enter n\n");
scanf("%d",&n);
tower(n,'s','t','d');
getch();
}
